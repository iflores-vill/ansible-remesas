<?xml version='1.0' encoding='UTF-8'?>
<agent-input>
  <variables>
    <variable name='sharedLocation' value='/opt/IBM/IMShared'/>
  </variables>
  <server>
    <repository location='/opt/installers/supplements'/>
    <repository location='/opt/installers/was'/>
    <repository location='/opt/installers/wct'/>
    <repository location='/opt/installers/fixsupp'/>
    <repository location='/opt/installers/fixwas'/>
    <repository location='/opt/installers/sdk'/>
  </server>
  <profile id='IBM HTTP Server V8.5' installLocation='/ihs'>
    <data key='cic.selector.arch' value='x86'/>
    <data key='user.wasjava' value='java8'/>
    <data key='user.ihs.http.server.service.name' value='none'/>
    <data key='user.ihs.httpPort' value='80'/>
    <data key='user.ihs.installHttpService' value='false'/>
    <data key='user.internal.use.only.prev.wasjava' value='java8'/>
  </profile>
  <install>
    <!-- IBM HTTP Server for WebSphere Application Server 8.5.5.23 -->
    <offering profile='IBM HTTP Server V8.5' id='com.ibm.websphere.IHS.v85' version='8.5.5023.20230116_1145' features='core.feature,arch.64bit'/>
  </install>
  <profile id='IBM WebSphere Application Server V8.5' installLocation='/was'>
    <data key='cic.selector.arch' value='x86'/>
    <data key='user.wasjava' value='java8'/>
    <data key='user.internal.use.only.prev.wasjava' value='java8'/>
  </profile>
  <install>
    <!-- IBM WebSphere Application Server Network Deployment 8.5.5.23 -->
    <offering profile='IBM WebSphere Application Server V8.5' id='com.ibm.websphere.ND.v85' version='8.5.5023.20230116_1145' features='core.feature,ejbdeploy,thinclient,embeddablecontainer,com.ibm.sdk.6_64bit'/>
  </install>
  <profile id='Web Server Plug-ins for IBM WebSphere Application Server V8.5' installLocation='/plugins'>
    <data key='cic.selector.arch' value='x86'/>
    <data key='user.wasjava' value='java8'/>
    <data key='user.internal.use.only.prev.wasjava' value='java8'/>
  </profile>
  <install>
    <!-- Web Server Plug-ins for IBM WebSphere Application Server 8.5.5.23 -->
    <offering profile='Web Server Plug-ins for IBM WebSphere Application Server V8.5' id='com.ibm.websphere.PLG.v85' version='8.5.5023.20230116_1145' features='core.feature,com.ibm.jre.6_64bit'/>
  </install>
  <profile id='WebSphere Customization Toolbox V8.5' installLocation='/Toolbox'>
    <data key='cic.selector.arch' value='x86'/>
    <data key='user.wasjava' value='java8'/>
    <data key='user.internal.use.only.prev.wasjava' value='java8'/>
  </profile>
  <install>
    <!-- WebSphere Customization Toolbox  8.5.5.23 -->
    <offering profile='WebSphere Customization Toolbox V8.5' id='com.ibm.websphere.WCT.v85' version='8.5.5023.20230116_1145' features='core.feature,pct,zpmt,zmmt'/>
  </install>
  <preference name='com.ibm.cic.common.core.preferences.eclipseCache' value='${sharedLocation}'/>
  <preference name='offering.service.repositories.areUsed' value='false'/>
</agent-input>
